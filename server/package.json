{
  "name": "server",
  "module": "index.ts",
  "type": "module",
  "engines": {
    "node": ">=16.0.0",
    "bun": ">=1.0.0"
  },
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "start": "bun run dist/index.js",
    "build": "bun build src/index.ts --outdir ./dist --target node --minify",
    "postinstall": "prisma generate",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "format": "prettier --write \"**/*.{ts,tsx}\"",
    "lint": "eslint . --ext .ts"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@hono/zod-validator": "^0.4.3",
    "@mswjs/interceptors": "^0.37.6",
    "@prisma/client": "^6.4.1",
    "@types/qrcode": "^1.5.5",
    "@types/speakeasy": "^2.0.10",
    "aws-sdk": "^2.1692.0",
    "bcrypt": "^5.1.1",
    "hone": "^1.1.0",
    "hono": "^4.7.4",
    "jose": "^6.0.8",
    "mock-aws-s3": "^4.0.2",
    "nock": "^14.0.1",
    "prisma": "^6.4.1",
    "qrcode": "^1.5.4",
    "resend": "^4.1.2",
    "speakeasy": "^2.0.0"
  }
}